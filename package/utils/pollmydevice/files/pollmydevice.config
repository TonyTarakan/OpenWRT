config interface '0'
        option mode 'server'
        option devicename '/dev/ttyAPP1'
        option baudrate '9600'
        option bytesize '8'
        option parity 'none'
        option flowcontrol 'none'
        option stopbits '1'
        option server_port '5002'
        option holdconntime '60'
        option client_port '6008'
        option client_host 'hub.m2m24.ru'
        option client_auth '0'
        option client_timeout '600'
        option modbus_gateway '0'

config interface '1'
        option mode 'disabled'
        option devicename '/dev/ttyAPP4'
        option baudrate '9600'
        option bytesize '8'
        option parity 'none'
        option flowcontrol 'none'
        option stopbits '1'
        option server_port '33001'
        option holdconntime '60'
        option client_port '6008'
        option client_host 'hub.m2m24.ru'
        option client_auth '1'
        option client_timeout '600'
        option modbus_gateway '0'

/dts-v1/;
#include "imx28.dtsi"
/ {
	model = "TELEOFIS";
	compatible = "i2se,duckbill", "fsl,imx28";

	memory {
		reg = <0x40000000 0x08000000>;
	};

	apb@80000000 {
		apbh@80000000 
		{
			gpmi-nand@8000c000 
			{
				pinctrl-names = "default";
				pinctrl-0 = <&gpmi_pins_0 &gpmi_pins_1>;
				status = "okay";
			};
			pinctrl@80018000 
			{
				pinctrl-names = "default";
				pinctrl-0 = <&hog_pins>;
				auart1_pins: auart1-pins@0 				/* RS485 */
				{
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_AUART1_RX__AUART1_RX
						MX28_PAD_AUART1_TX__AUART1_TX

					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};
				auart4_pins: auart4-pins@0 				/* RS232 */
				{
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_SAIF0_MCLK__AUART4_CTS
						MX28_PAD_SAIF0_LRCLK__AUART4_RTS
						MX28_PAD_SAIF0_BITCLK__AUART4_RX
						MX28_PAD_SAIF0_SDATA0__AUART4_TX
					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};
				duart_pins: duart-pins@0 {				/* for fake console */
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_PWM0__DUART_RX
						MX28_PAD_PWM1__DUART_TX
					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};
				watchdog: watchdog@0 {
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_PWM4__GPIO_3_29		/* LED.TOP/WATCHDOG*/
					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};
				gpio_pins_3V3: gpio-pins-3V3@0 {
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_SSP0_DATA5__GPIO_2_5		/* GSM.SIMA */
						MX28_PAD_SSP1_CMD__GPIO_2_13		/* GSM.SIMDET */
						MX28_PAD_SSP2_SCK__GPIO_2_16		/* SIMDET.1# */
						MX28_PAD_SSP3_SCK__GPIO_2_24		/* SIMDET.0# */
						MX28_PAD_SSP3_SS0__GPIO_2_27		/* GSM.VCCEN# */
						MX28_PAD_PWM3__GPIO_3_28			/* USBHOST.VCCEN# */
						MX28_PAD_SSP2_SS0__GPIO_2_19		/* PCI.VCCEN# */
						/* ADDITIONAL GPIO */
						MX28_PAD_I2C0_SDA__GPIO_3_25		
						MX28_PAD_SAIF1_SDATA0__GPIO_3_26
						MX28_PAD_SSP0_DATA0__GPIO_2_0	
						MX28_PAD_SSP0_SCK__GPIO_2_10
						MX28_PAD_SSP0_DATA6__GPIO_2_6
						MX28_PAD_SSP0_DATA7__GPIO_2_7
						MX28_PAD_SSP0_DATA3__GPIO_2_3
						MX28_PAD_SSP0_DATA4__GPIO_2_4
						MX28_PAD_SSP1_SCK__GPIO_2_12
					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};

				/* NAND */
				gpmi_pins_0: gpmi-pins-0@0 {
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_GPMI_ALE__GPMI_ALE
						MX28_PAD_GPMI_CLE__GPMI_CLE
						MX28_PAD_GPMI_CE0N__GPMI_CE0N
						MX28_PAD_GPMI_D00__GPMI_D0
						MX28_PAD_GPMI_D01__GPMI_D1
						MX28_PAD_GPMI_D02__GPMI_D2
						MX28_PAD_GPMI_D03__GPMI_D3
						MX28_PAD_GPMI_D04__GPMI_D4
						MX28_PAD_GPMI_D05__GPMI_D5
						MX28_PAD_GPMI_D06__GPMI_D6
						MX28_PAD_GPMI_D07__GPMI_D7
						MX28_PAD_GPMI_RDN__GPMI_RDN
						MX28_PAD_GPMI_RESETN__GPMI_RESETN
						MX28_PAD_GPMI_WRN__GPMI_WRN
					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};
				gpmi_pins_1: gpmi-pins-1@0 {
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_GPMI_RDY0__GPMI_READY0
					>;
					fsl,drive-strength = <MXS_DRIVE_8mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_ENABLE>;
				};
				hog_pins: hog-pins-high@0 {
					reg = <0>;
					fsl,pinmux-ids = <
						MX28_PAD_SSP1_DATA0__GPIO_2_14		/* ETH.RES# workaround init*/
					>;
					fsl,drive-strength = <MXS_DRIVE_4mA>;
					fsl,voltage = <MXS_VOLTAGE_HIGH>;
					fsl,pull-up = <MXS_PULL_DISABLE>;
				};
			};
			ocotp: ocotp@8002c000 
			{
				compatible = "fsl,imx28-ocotp", "fsl,ocotp";
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x8002c000 0x2000>;
				clocks = <&clks 25>;
				status = "okay";
			};
		};
		apbx@80040000 {
			power@80044000 {
				status = "okay";
				reg_vddd: regulator@80044040 {
					status = "okay";
				};
				reg_vdda: regulator@80044050 {
					status = "okay";
				};
				reg_vddio: regulator@80044060 {
					status = "okay";
				};
			};
			lradc@80050000 {
				fsl,lradc-touchscreen-wires = <4>;
				status = "okay";
				fsl,lradc-touchscreen-wires = <4>;
				fsl,ave-ctrl = <4>;
				fsl,ave-delay = <2>;
				fsl,settling = <10>;
			};
			auart1: serial@8006c000 {
				pinctrl-names = "default";
				pinctrl-0 = <&auart1_pins>;
				status = "okay";
			};
			auart4: serial@80072000 {
				pinctrl-names = "default";
				pinctrl-0 = <&auart4_pins>;
				status = "okay";
			};
			duart: serial@80074000 {
				pinctrl-names = "default";
				pinctrl-0 = <&duart_pins>;
				status = "okay";
			};
			usbphy0: usbphy@8007c000 {
				status = "okay";
			};
			usbphy1: usbphy@8007e000 {
				status = "okay";
			};
		};
	};
	ahb@80080000 {
		usb0: usb@80080000 {
			pinctrl-names = "default";
			pinctrl-0 = <&usb0_pins_a>;	/* pinctrl-0 = <&usb0_pins_a>; - normal  |||||   pinctrl-0 = <&usb0_pins_a &usb0_id_pins_a>; - gadget */
			vbus-supply = <&reg_usb0_vbus>;
			status = "okay";
		};
		usb1: usb@80090000 {
			pinctrl-names = "default";
			pinctrl-0 = <&usb1_pins_a>;
			vbus-supply = <&reg_usb1_vbus>;
			status = "okay";
		};
		mac0: ethernet@800f0000 {
			phy-mode = "rmii";
			pinctrl-names = "default";
			pinctrl-0 = <&mac0_pins_a>;
			phy-supply = <&reg_3p3v>;
			phy-reset-gpios = <&gpio2 14 GPIO_ACTIVE_LOW>;
			phy-reset-duration = <200>;
			status = "okay";
		};
		mac1: ethernet@800f4000 {
			phy-mode = "rmii";
			pinctrl-names = "default";
			pinctrl-0 = <&mac1_pins_a>;
			phy-supply = <&reg_3p3v>;
			status = "okay";
		};
	};
	gpio {
		compatible = "fsl,imx28-gpio", "fsl,mxs-gpio";
		pinctrl-names = "default";
		/*pinctrl-0 = <&gpio_pins_1V8 &gpio_pins_3V3>;*/
		pinctrl-0 = <&gpio_pins_3V3>;
		gpio-controller;
		#gpio-cells = <2>;
		gpio_GSM_SIMA {
			label = "gsm:simnumber";
			gpios = <&gpio2 5 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		gpio_GSM_SIMDET {					/* out to modem */
			label = "gsm:simdet";
			gpios = <&gpio2 13 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		gpio_GSM_SIMDET_0 {
			label = "gsm:simdet0";
			input;
			gpios = <&gpio2 24 GPIO_ACTIVE_LOW>;
		};
		gpio_GSM_SIMDET_1 {
			label = "gsm:simdet1";
			gpios = <&gpio2 16 GPIO_ACTIVE_LOW>;
			input;
		};
		gpio_GSM_VCCEN# {
			label = "gsm:vccen";
			gpios = <&gpio2 27 GPIO_ACTIVE_LOW>;
			output-low;
		};
		gpio_USBHOST_VCCEN {
			gpio-hog;
			label = "usb1:vccen";
			gpios = <&gpio3 28 GPIO_ACTIVE_HIGH>;
			output-high;
			line-name = "usbhost_vccen";
		};
		gpio_PCI_VCCEN# {
			label = "pci:vccen";
			gpios = <&gpio2 19 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_0_PULLUP# {
			label = "0:pullup";
			gpios = <&gpio2 7 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_0_PULLDOWN {
			label = "0:pulldown";
			gpios = <&gpio2 3 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_1_PULLUP# {
			label = "1:pullup";
			gpios = <&gpio2 4 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_1_PULLDOWN {
			label = "1:pulldown";
			gpios = <&gpio2 6 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_2_PULLUP# {
			label = "2:pullup";
			gpios = <&gpio2 10 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_2_PULLDOWN {
			label = "2:pulldown";
			gpios = <&gpio3 25 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_3_PULLUP# {
			label = "3:pullup";
			gpios = <&gpio2 0 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
		gpio_3_PULLDOWN {
			label = "3:pulldown";
			gpios = <&gpio3 26 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};
	watchdog {
		/* ADM706 */
		compatible = "linux,wdt-gpio";
		pinctrl-names = "default";
		pinctrl-0 = <&watchdog>;
		gpios = <&gpio3 29 GPIO_ACTIVE_HIGH>;
		hw_algo = "toggle";
		hw_margin_ms = <8000>;
		always-running = <1>;
	};
	regulators {
		compatible = "simple-bus";

		reg_3p3v: 3p3v {
			compatible = "regulator-fixed";
			regulator-name = "3P3V";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};
		reg_usb0_vbus: usb0_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb0_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};
		reg_usb1_vbus: usb1_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb1_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};
	};
};
